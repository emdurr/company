<h1 class="center-align card-panel blue-text text-darken-2 card-panel teal lighten-2 ">All the Pets</h1>
<% @pets.each do |pet| %>
	<h4><%= link_to pet.name, pet_path(pet) %></h4>
	<ul>
		<li><%= pet.breed %></li>
		<li><%= pet.fur_color %></li>
		<li><%= pet.fur_type %></li>
		<li><%= pet.gender %></li>
		<li><%= pet.age %></li>
	</ul>
<% end %>

<%= link_to 'Home', root_path %>